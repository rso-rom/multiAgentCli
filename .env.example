# Logging
LOG_LEVEL=info  # debug | info | warn | error | none

# Model Backend
MODEL_BACKEND=mock
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama3
OPENWEBUI_URL=http://localhost:3000/api/v1/chat/completions
OPENWEBUI_API_KEY=
OPENWEBUI_MODEL=llama3
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
EXECUTION_MODE=host
DOCKER_RUNTIME_IMAGE=python:3.12-slim

# Memory System
MEMORY_PATH=./memory
USE_QDRANT=false
QDRANT_URL=http://localhost:6333

# Embedding Service (for Qdrant semantic search)
EMBEDDING_SERVICE=ollama
EMBEDDING_MODEL=nomic-embed-text
EMBEDDING_DIMENSION=768

# Ask-Store (prompt history)
ASK_STORE_ENABLED=true

# Auto-Resume (on token limits)
AUTO_RESUME_ENABLED=true
MAX_RESUME_ATTEMPTS=3

# Tool Auto-Installation
AUTO_INSTALL_TOOLS=ask
